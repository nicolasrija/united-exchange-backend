{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true,
    "jest/globals": true
  },
  "extends": ["eslint:recommended", "google"],
  "plugins": ["prettier", "jest"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "object-curly-spacing": 0,
    "prettier/prettier": "error",
    "indent": 0,
    "comma-dangle": 0,
    "max-len": ["error", { "code": 120 }]
  }
}
